  <remap to="/odom" from="/input_odom"/>
</node>

<node pkg="first_project" name="lidar_select" type="lidar_select" ></node>

 <node pkg="rosbag" type="play" name="loop" args=" -l /home/polimi/robotics/src/bags/robotics.bag -r 1" required="true" output="screen"></node>
